<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_check_book_overdue" model="ir.cron">
        <field name="name">Library: Check Overdue Books Daily</field>
        <field name="model_id" ref="model_library_book_issue"/>
        <field name="state">code</field>
        <field name="code">model._cron_check_overdue()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_report_book_issue_card" model="ir.actions.report">
        <field name="name">Book Issue Card</field>
        <field name="model">library.member</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">library_management.report_book_issue_card</field>
        <field name="report_file">library_management.report_book_issue_card</field>
        <field name="print_report_name">'Issue Card - %s' % (object.name)</field>
        <field name="binding_model_id" ref="model_library_member"/>
        <field name="binding_type">report</field>
    </record>


    <record id="action_report_member_xlsx" model="ir.actions.report">
        <field name="name">Member Excel Report</field>
        <field name="model">library.member</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">library_management.report_member_xlsx</field>
        <field name="report_file">Member_Report</field>
        <field name="binding_model_id" ref="model_library_member"/>
        <field name="binding_type">report</field>
    </record>

</odoo>